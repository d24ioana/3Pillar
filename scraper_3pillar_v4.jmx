              <stringProp name="parameters">${__UUID}</stringProp>
              <stringProp name="script">var guid=args[0];
log.info(guid.toString());
var k = vars.get(&quot;k&quot;);
log.info(k.toString());
var title = vars.get(&quot;titles&quot;);
log.info(title);
var id = vars.get(&quot;idss&quot;);
var link = id;
log.info(link);
var remote = vars.get(&quot;remote&quot;);
var country = vars.get(&quot;countries&quot;);
var city = vars.get(&quot;cities&quot;);
if(city.contains(&apos;Romania&apos;) == true){
	//city == &quot;&quot;;
	city = city.replaceAll(&apos;Romania&apos;,&apos;Remote&apos;);
	}else{
		remote = &apos;&apos;;}
log.info(city);
var company = vars.get(&quot;company&quot;);
String job=&apos;{&quot;id&quot;: &quot;&apos;+guid+&apos;&quot;, &quot;remote&quot;: &quot;&apos;+remote+&apos;&quot;, &quot;job_title&quot;: &quot;&apos;+title+&apos;&quot;,&quot;job_link&quot;: &quot;&apos;+link+&apos;&quot;, &quot;company&quot;: &quot;&apos;+company+&apos;&quot;,    &quot;country&quot;: &quot;&apos;+country+&apos;&quot;,&quot;city&quot;: &quot;&apos;+city+&apos;&quot; }&apos;;
var joblist = vars.getObject(&quot;joblist&quot;);
joblist = joblist + job;
vars.putObject(&quot;joblist&quot;, joblist);
</stringProp>
